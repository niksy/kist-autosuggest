{
  "name": "kist-autosuggest",
  "version": "0.5.0",
  "description": "Simple autosuggest plugin.",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com)",
  "license": "MIT",
  "files": [
    "index.cjs.js",
    "index.esm.js",
    "LICENSE.md",
    "README.md"
  ],
  "sideEffects": false,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "eslint '{index,lib/**/*}.js'",
    "prepublishOnly": "npm run build",
    "release": "np",
    "test": "npm run lint",
    "test:generate-static-site": "BABEL_ENV=test webpack --config ./test/manual/webpack.config.js",
    "test:generate-static-site:watch": "BABEL_ENV=test webpack-dev-server --config ./test/manual/webpack.config.js",
    "test:manual": "concurrently -k \"nodemon test/server/server.js\" \"npm run test:generate-static-site:watch\""
  },
  "dependencies": {
    "jquery": "^1.12.4",
    "throttle-debounce": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "babel-loader": "^8.0.4",
    "babel-preset-niksy": "^4.1.0",
    "concurrently": "^4.1.0",
    "cors": "^2.3.1",
    "css-loader": "^2.1.0",
    "del": "^2.2.0",
    "eslint": "^5.4.0",
    "eslint-config-niksy": "^6.1.0",
    "eslint-plugin-extend": "^0.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-unicorn": "^5.0.0",
    "express": "^4.4.5",
    "globby": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mustache": "^0.8.2",
    "nodemon": "^1.18.10",
    "np": "^3.0.4",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^3.0.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "suitcss-components-test": "^1.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "autocomplete",
    "autosuggest",
    "ecosystem:jquery",
    "jquery-plugin",
    "kist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niksy/kist-autosuggest.git"
